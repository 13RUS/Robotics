    "def calculate_robot_speed(speed, commands):\n",
    "    \n",
    "    for command in commands:\n",
    "        if command == 'A':\n",
    "            speed += 1\n",
    "        else:\n",
    "            speed -=1\n",
    "     \n",
    "    return speed\n",
    "\n",
    "speed = int(input())\n",
    "commands = input()\n",
    "\n",
    "print (calculate_robot_speed(speed, commands))"
   ]
  }
